import Reactium from 'reactium-core/sdk';

(async () => {
    Reactium.Hook.register('plugin-ready', async () => {
        const { default: Element } = await import('.');
        if (!Reactium.Toolkit) return;
        
        // Element {{name}}
        Reactium.Toolkit.Elements.register('{{id}}-element', {
            zone: '{{zone}}',
            component: Element,
            order: Reactium.Enums.priority.low,
        });
    });
})();
